{
  "name": "boltbucket",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"cd client && vite\" \"cd server && npm run reset && nodemon server\"",
    "start": "cd server && node server",
    "build": "cd client && vite build",
    "reset": "cd server && cd config && node --require dotenv/config reset.js"
  },
  "dependencies": {
    "@picocss/pico": "^1.5.7",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.21.1",
    "nodemon": "^2.0.21",
    "pg": "^8.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.9.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/serve-favicon": "^2.5.7",
    "@vitejs/plugin-react": "^3.1.0",
    "vite": "^4.2.0"
  }
}
